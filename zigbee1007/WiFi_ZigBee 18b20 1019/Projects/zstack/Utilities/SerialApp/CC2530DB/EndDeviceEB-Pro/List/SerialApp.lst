    899              uint16 shortAddr;
    900              uint8 strBuf[11]={0};  
    901              
    902              SerialApp_TxAddr.addrMode = (afAddrMode_t)Addr16Bit;
    903              SerialApp_TxAddr.endPoint = SERIALAPP_ENDPOINT;
