    907              
    908              strBuf[0] = 0x3B;                          //发送地址给协调器 可用于点播
    909              strBuf[1] = HI_UINT16( shortAddr );        //存放短地址高8位
    910              strBuf[2] = LO_UINT16( shortAddr );        //存放短地址低8位
    911              
    912              osal_memcpy(&strBuf[3], NLME_GetExtAddr(), 8);
