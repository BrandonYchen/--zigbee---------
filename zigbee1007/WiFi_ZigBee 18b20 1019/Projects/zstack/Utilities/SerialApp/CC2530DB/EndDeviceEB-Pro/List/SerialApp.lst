    929          void GetIeeeAddr(uint8 * pIeeeAddr, uint8 *pStr)
    930          {
    931            uint8 i;
    932            uint8 *xad = pIeeeAddr;
    933          
    934            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    935            {
    936              uint8 ch;
    937              ch = (*xad >> 4) & 0x0F;
    938              *pStr++ = ch + (( ch < 10 ) ? '0' : '7');
    939              i++;
    940              ch = *xad & 0x0F;
    941              *pStr++ = ch + (( ch < 10 ) ? '0' : '7');
    942              i++;
    943            }
    944          }
